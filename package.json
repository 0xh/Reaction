{
  "name": "reaction",
  "author": "James Smith",
  "version": "1.0.64",
  "license": "MIT",
  "repository": "https://github.com/djalbat/Reaction",
  "dependencies": {
    "easyui": "^3.2.26"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-plugin-transform-react-jsx": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "browserify": "^13.0.0",
    "chokidar-cli": "^1.2.0",
    "npm-update-outdated": "^0.1.6"
  },
  "scripts": {
    "clean:lib": "rm -rf ./lib/*",
    "clean:docs": "rm -rf ./docs/lib/*",
    "babel:lib": "babel ./libES2015 --presets=es2015 --source-maps inline --out-dir=./lib",
    "babel:docs": "babel ./docs/libES2015 --presets es2015,react --plugins transform-react-jsx --source-maps inline --out-dir ./docs/lib",
    "browserify:docs": "browserify ./docs/index.js --standalone example --debug > ./docs/example.js",
    "build:lib": "npm run clean:lib && npm run babel:lib",
    "build:docs": "npm run clean:docs && npm run babel:docs && npm run browserify:docs",
    "watch:lib": "chokidar './index.js' './libES2015/**/*.js' -c 'npm run build:lib'",
    "watch:docs": "chokidar './docs/index.js' './docs/libES2015/**/*.js' -c 'npm run build:docs'",
    "update-outdated": "npm-update-outdated",
    "git": "./scripts/git.sh",
    "build": "npm run build:lib && npm run build:docs",
    "watch": "npm run watch:lib & npm run watch:docs"
  }
}
